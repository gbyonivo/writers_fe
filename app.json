{
  "expo": {
    "name": "writers_fe",
    "slug": "writers_fe",
    "version": "1.0.16",
    "orientation": "portrait",
    "icon": "./src/assets/images/app/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "ivo.tertsegha",
      "buildNumber": "16",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Writers to access your camera",
        "NSMicrophoneUsageDescription": "Allow Writers to access your microphone"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/images/app/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "09c52d3c-4a82-4694-99a0-bd7547a0d98b"
      }
    },
    "scheme": "writersfe",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/09c52d3c-4a82-4694-99a0-bd7547a0d98b"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Poppins-Bold.ttf",
            "./src/assets/fonts/Poppins-ExtraLight.ttf",
            "./src/assets/fonts/Poppins-Light.ttf",
            "./src/assets/fonts/Poppins-Medium.ttf",
            "./src/assets/fonts/Poppins-Regular.ttf",
            "./src/assets/fonts/Poppins-SemiBold.ttf",
            "./src/assets/fonts/Poppins-Thin.ttf"
          ]
        }
      ],
      "expo-asset",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Writers to access your camera",
          "microphonePermission": "Allow Writers to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
